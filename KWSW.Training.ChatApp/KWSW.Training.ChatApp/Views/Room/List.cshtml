@model IEnumerable<KWSW.Training.CharApp.Contracts.Room>
@{
    ViewData["Title"] = "Chat rooms";
}
<h1>@ViewData["Title"]</h1>

<ul>
    @{
        foreach (var room in Model)
        {
            <li>@Html.DisplayFor(m => room.Name) @Html.ActionLink("go to chat", "Chat", new {roomId = room.Id}) </li>
        }
    }
</ul>
